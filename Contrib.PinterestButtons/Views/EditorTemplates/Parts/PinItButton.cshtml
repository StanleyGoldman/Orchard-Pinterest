@model Contrib.PinterestButtons.Models.PinItButtonPart
<fieldset>
    <legend>Pin it Fields</legend>

    <div class="editor-label">
        @Html.LabelFor(model => model.PinCount)
    </div>
    <div class="editor-field">
        @Html.TextBoxFor(model => model.PinCount)
        @Html.ValidationMessageFor(model => model.PinCount)
    </div>

    <div class="editor-label">
        @Html.LabelFor(model => model.AnyImage)
    </div>
    <div class="editor-field">
        @Html.CheckBoxFor(model => model.AnyImage)
        @Html.ValidationMessageFor(model => model.AnyImage)
    </div>
    
    <div class="editor-label">
        @Html.LabelFor(model => model.Url)
    </div>
    <div class="editor-field">
        @Html.TextBoxFor(model => model.Url)
        @Html.ValidationMessageFor(model => model.Url)
    </div>
    
    <div class="editor-label">
        @Html.LabelFor(model => model.Image)
    </div>
    <div class="editor-field">
        @Html.TextBoxFor(model => model.Image)
        @Html.ValidationMessageFor(model => model.Image)
    </div>
    
    <div class="editor-label">
        @Html.LabelFor(model => model.Description)
    </div>
    <div class="editor-field">
        @Html.TextBoxFor(model => model.Description)
        @Html.ValidationMessageFor(model => model.Description)
    </div>
</fieldset>
